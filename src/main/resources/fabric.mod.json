{
  "schemaVersion": 1,
  "id": "seedmapper",
  "version": "${version}",

  "name": "SeedMapper",
  "description": "In-game Minecraft Fabric mod that allows you to do various things with the world seed.",
  "authors": [
    "xpple"
  ],
  "contact": {
    "homepage": "https://xpple.dev/",
    "sources": "https://github.com/xpple/SeedMapper/"
  },

  "license": "LGPL-3.0-or-later",
  "icon": "assets/seedmapper/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "dev.xpple.seedmapper.SeedMapper"
    ]
  },
  "mixins": [
    "seedmapper.mixin.advancement.json",
    "seedmapper.mixin.client.json",
    "seedmapper.mixin.fabric.json",
    "seedmapper.mixin.recipe.json",
    "seedmapper.mixin.render.json",
    "seedmapper.mixin.server.json"
  ],
  "accessWidener": "seedmapper.aw",

  "depends": {
    "fabricloader": ">=0.14.17",
    "fabric-api": ">=0.37.1",
    "minecraft": "1.20.1"
  }
}
